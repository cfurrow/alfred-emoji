{
  "version": "1.11.0",
  "engines": {
    "npm": ">=7.8.0",
    "node": ">=16.0.0"  
  },
  "scripts": {
    "webpack": "webpack",
    "clean": "rm -f *.alfredworkflow; rm -rf output/*",
    "build": "./build.sh",
    "load": "npm run build && open alfred-emoji.alfredworkflow",
    "test": "tap 'test/**/*.test.js'",
    "test:watch": "npm test -- --watch",
    "test-ci": "tap --cov 'test/**/*.test.js'",
    "lint": "standard | snazzy",
    "lint-ci": "standard"
  },
  "precommit": [
    "lint",
    "test"
  ],
  "dependencies": {
    "emojilib": "^3.0.4",
    "unicode-emoji-json": "^0.3.0",
    "fontkit": "^1.8.1"
  },
  "devDependencies": {
    "mock-require": "^3.0.3",
    "pre-commit": "^1.2.2",
    "snazzy": "^9.0.0",
    "standard": "^16.0.4",
    "tap": "^15.1.6",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
